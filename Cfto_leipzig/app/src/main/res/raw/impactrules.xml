<?xml version="1.0" encoding="UTF-8"?>
<WeatherConditionRules>

	<ruleset mode="intensity">
		<rule des="Light" impact="0"/>
		<rule des="Moderate" impact="1"/>
		<rule des="Heavy" impact="2"/>
	</ruleset>

	<ruleset mode="descriptor">
		<rule des="Shallow" impact="0"/>
		<rule des="Partial" impact="0"/>
		<rule des="Patches" impact="0"/>
		<rule des="LowDrifting" impact="0"/>
		<rule des="Blowing" impact="0"/>
		<rule des="Showers" impact="1"/>
		<rule des="Thunderstorms" impact="2"/>
		<rule des="Freezing" impact="2"/>
	</ruleset>

	<ruleset mode="phenomena">
		<rule des="Drizzle" impact="0"/>
		<rule des="Rain" impact="1"/>
		<rule des="Snow" impact="2"/>
		<rule des="SnowGrains" impact="1"/>
		<rule des="IceCrystals" impact="1"/>
		<rule des="IcePellets" impact="1"/>
		<rule des="Hail" impact="2"/>
		<rule des="SmallHail" impact="1"/>
		<rule des="UnknownPrecpitation" impact="0"/>
		<rule des="Mt" impact="1"/>
		<rule des="Fog" impact="1"/>
		<rule des="Smoke" impact="2"/>
		<rule des="VolcanicAsh" impact="2"/>
		<rule des="WidespreadDust" impact="1"/>
		<rule des="Sand" impact="0"/>
		<rule des="Haze" impact="1"/>
		<rule des="Spray" impact="1"/>
		<rule des="DustSandWhirls" impact="2"/>
		<rule des="Squalls" impact="2"/>
		<rule des="Sandstorm" impact="2"/>
		<rule des="Duststorm" impact="2"/>
		<rule des="FunnelCloud" impact="2"/>
		<rule des="Tornado" impact="2"/>
		<rule des="Waterspout" impact="2"/>
	</ruleset>

</WeatherConditionRules>



